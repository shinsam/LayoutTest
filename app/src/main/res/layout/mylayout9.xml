<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/btn_91"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:background="#673AB7"
        android:text="1"
        android:textSize="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.515" />

    <Button
        android:id="@+id/btn_92"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="2"
        android:textSize="40dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_91"
        app:layout_constraintTop_toTopOf="@+id/btn_91" />


    <Button
        android:id="@+id/btn_93"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="3"
        android:textSize="40dp"
        app:layout_constraintStart_toEndOf="@+id/btn_91"
        app:layout_constraintTop_toTopOf="@+id/btn_91" />

    <Button
        android:id="@+id/btn_94"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="4"
        android:textSize="40dp"
        app:layout_constraintBottom_toTopOf="@+id/btn_93"
        app:layout_constraintEnd_toEndOf="@+id/btn_93" />

    <Button
        android:id="@+id/btn_95"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="5"
        android:textSize="40dp"
        app:layout_constraintStart_toStartOf="@+id/btn_92"
        app:layout_constraintTop_toBottomOf="@+id/btn_92" />

</androidx.constraintlayout.widget.ConstraintLayout>